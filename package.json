{
  "name": "qards.link",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "prisma generate && next dev -p 11000",
    "build": "prisma generate && prisma db push && next build",
    "format:write": "prettier --write \"**/*.{css,js,json,jsx,ts,tsx}\"",
    "format": "prettier \"**/*.{css,js,json,jsx,ts,tsx}\"",
    "start": "next start",
    "lint": "next lint",
    "deploy": "pnpm i && now --prod",
    "emails": "email dev",
    "backup-data": "env $(grep -v '^#' .env | sed 's/\"//g' | xargs) bash -c 'pg_dump --data-only \"$POSTGRES_PRISMA_URL\" | gzip > backup/instaqards_com_data_$(date +%Y-%m-%d_%H-%M-%S).sql.gz'",
    "backup-schema": "env $(grep -v '^#' .env | sed 's/\"//g' | xargs) bash -c 'pg_dump --clean --schema-only \"$POSTGRES_PRISMA_URL\" | gzip > backup/instaqards_com_schema_$(date +%Y-%m-%d_%H-%M-%S).sql.gz'",
    "backup": "env $(grep -v '^#' .env | sed 's/\"//g' | xargs) bash -c 'pg_dump --clean \"$POSTGRES_PRISMA_URL\" | gzip > backup/instaqards_com_$(date +%Y-%m-%d_%H-%M-%S).sql.gz'",
    "restore": "env $(grep -v '^#' .env | sed 's/\"//g' | xargs) bash -c 'gunzip -c \"$1\" | psql \"$POSTGRES_PRISMA_URL\"' --"
  },
