    "dev:restore": "env $(grep -v '^#' .env | sed 's/\"//g' | xargs) bash -c 'gunzip -c \"$1\" | psql \"$DATABASE_URL\"' --",
    "dev:restore-sql": "env $(grep -v '^#' .env | sed 's/\"//g' | xargs) bash -c 'psql \"$DATABASE_URL\" < \"$1\"' --"
